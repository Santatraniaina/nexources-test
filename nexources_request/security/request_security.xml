<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record model="res.groups.privilege" id="res_groups_privilege_request">
        <field name="name">Requests</field>
        <field name="category_id" ref="base.module_category_services"/>
    </record>

    <record id="group_request_user" model="res.groups">
        <field name="name">User</field>
        <field name="sequence">10</field>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        <field name="privilege_id" ref="res_groups_privilege_request"/>
    </record>

    <record id="group_request_manager" model="res.groups">
        <field name="name">Administrator</field>
        <field name="sequence">20</field>
        <field name="privilege_id" ref="res_groups_privilege_request"/>
        <field name="implied_ids" eval="[(4, ref('group_request_user'))]"/>
        <field name="user_ids" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>
</odoo>